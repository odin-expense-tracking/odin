<div class="page-header">
  <%= link_to guides_texts_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    All Guides_texts
  <% end %>
  <%= link_to edit_guides_text_path(@guides_text), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show guides_text</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @guides_text.title %></dd>

  <dt>User:</dt>
  <dd><%= @guides_text.user %></dd>

  <dt>Published at:</dt>
  <dd><%= @guides_text.published_at %></dd>

</dl>
