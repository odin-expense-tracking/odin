        <div class="dialog-box">
          <div class="dialog-box-header">
            <div class="dialog-box-title text-center">Login</div>
          </div>
          <%= form_for '' do |f| %>
            <div class="dialog-box-body">
              <div class="form-field">
                <%= f.label :email %>
                <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
              </div>
              <div class="form-field">
                <%= f.label :password %>
                <%= f.password_field :password, autocomplete: "off" %>
              </div>
            </div>
            <div class="dialog-box-actions text-center">
              <%= f.submit "Log in", class: 'btn btn-primary', data: {disable_with: "<i class='fa fa-spinner fa-spin'></i> Logging in..."} %>
              <%= button_tag 'Cancel', class: 'btn btn-danger close' %>
            </div>
          <% end %>
        </div>
      </div>
